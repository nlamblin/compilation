          case 7: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [36] { return new Symbol(sym.PF, yytext()); }");
            { return new Symbol(sym.PF, yytext());
            }
          case 29: break;
          case 8: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [37] { return new Symbol(sym.ACO, yytext()); }");
            { return new Symbol(sym.ACO, yytext());
            }
          case 30: break;
          case 9: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [38] { return new Symbol(sym.ACF, yytext()); }");
            { return new Symbol(sym.ACF, yytext());
            }
          case 31: break;
          case 10: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [39] { return new Symbol(sym.DOT); }");
            { return new Symbol(sym.DOT);
            }
          case 32: break;
          case 11: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [46] { return new Symbol(sym.EGAL); }");
            { return new Symbol(sym.EGAL);
            }
          case 33: break;
          case 12: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [47] { return new Symbol(sym.SUP); }");
            { return new Symbol(sym.SUP);
            }
          case 34: break;
          case 13: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [48] { return new Symbol(sym.INF); }");
            { return new Symbol(sym.INF);
            }
          case 35: break;
          case 14: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [49] { return new Symbol(sym.COMP); }");
            { return new Symbol(sym.COMP);
            }
          case 36: break;
          case 15: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [50] { return new Symbol(sym.SUP_EGAL); }");
            { return new Symbol(sym.SUP_EGAL);
            }
          case 37: break;
          case 16: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [51] { return new Symbol(sym.INF_EGAL); }");
            { return new Symbol(sym.INF_EGAL);
            }
          case 38: break;
          case 17: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [52] { return new Symbol(sym.DIFF); }");
            { return new Symbol(sym.DIFF);
            }
          case 39: break;
          case 18: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [42] { return new Symbol(sym.INT, yytext()); }");
            { return new Symbol(sym.INT, yytext());
            }
          case 40: break;
          case 19: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [44] { return new Symbol(sym.DEFVAR, yytext()); }");
            { return new Symbol(sym.DEFVAR, yytext());
            }
          case 41: break;
          case 20: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [41] { return new Symbol(sym.VOID, yytext()); }");
            { return new Symbol(sym.VOID, yytext());
            }
          case 42: break;
          case 21: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [45] { return new Symbol(sym.AFFECT); }");
            { return new Symbol(sym.AFFECT);
            }
          case 43: break;
          case 22: 
            System.out.println("line: "+(yyline+1)+" "+"match: --"+zzToPrintable(yytext())+"--");
            System.out.println("action [40] { return new Symbol(sym.FUNCTION, yytext()); }");
            { return new Symbol(sym.FUNCTION, yytext());
            }
          case 44: break;
          default:
            zzScanError(ZZ_NO_MATCH);
        }
      }
    }
  }

  /**
   * Runs the scanner on input files.
   *
   * This main method is the debugging routine for the scanner.
   * It prints debugging information about each returned token to
   * System.out until the end of file is reached, or an error occured.
   *
   * @param argv   the command line, contains the filenames to run
   *               the scanner on.
   */
  public static void main(String argv[]) {
    if (argv.length == 0) {
      System.out.println("Usage : java Yylex [ --encoding <name> ] <inputfile(s)>");
    }
    else {
      int firstFilePos = 0;
      String encodingName = "UTF-8";
      if (argv[0].equals("--encoding")) {
        firstFilePos = 2;
        encodingName = argv[1];
        try {
          java.nio.charset.Charset.forName(encodingName); // Side-effect: is encodingName valid? 
        } catch (Exception e) {
          System.out.println("Invalid encoding '" + encodingName + "'");
          return;
        }
      }
      for (int i = firstFilePos; i < argv.length; i++) {
        Yylex scanner = null;
        try {
          java.io.FileInputStream stream = new java.io.FileInputStream(argv[i]);
          java.io.Reader reader = new java.io.InputStreamReader(stream, encodingName);
          scanner = new Yylex(reader);
          do {
            System.out.println(scanner.next_token());
          } while (!scanner.zzAtEOF);

        }
        catch (java.io.FileNotFoundException e) {
          System.out.println("File not found : \""+argv[i]+"\"");
        }
        catch (java.io.IOException e) {
          System.out.println("IO error scanning file \""+argv[i]+"\"");
          System.out.println(e);
        }
        catch (Exception e) {
          System.out.println("Unexpected exception:");
          e.printStackTrace();
        }
      }
    }
  }


}
