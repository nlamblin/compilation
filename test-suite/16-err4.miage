/*
Test de controle semantique 
 
 int i;
 
 void f(){
	int x;
	x=0;
 }
 
 void main(){
	i = i + f(); /* erreur : compatibilite de type */
 }
 
 Traduction
 */

 VAR i;
 
 FUNCTION VOID f(){
	VAR x;
	AFFECT x=0;
 }
 
 FUNCTION VOID main(){
	AFFECT i = i + f(); /* erreur : compatibilite de type */
 }
