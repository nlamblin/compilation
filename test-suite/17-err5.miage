/*
Test de controle semantique
 
 int i;
 
 void f(int a, int b){
	int x;
	x=0;
 }
 
 void main(){
	f(i); /* erreur : nombre de parametres */
 }
 
Traduction
*/

 VAR i;
 
 FUNCTION VOID f(a, b){
	VAR x;
	AFFECT x=0;
 }
 
 FUNCTION VOID main(){
	CALL f(i); /* erreur : nombre de parametres */
 }