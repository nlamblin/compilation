Traduction
*/

 VAR i = 0;
 VAR j;

 FUNCTION INT f(a,b) {
	VAR x;

	AFFECT x=(a*2)+b-5/3;
	
	RETURN x+2;
 }

 FUNCTION VOID main() {
	AFFECT j = read();
	write(f(i+1),j));
 }
